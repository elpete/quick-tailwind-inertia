    "repository":{
        "type":"",
        "url":""
    },
    "bugs":"",
    "shortDescription":"",
    "contributors":[],
    "ignore":[],
    "devDependencies":{
        "testbox":"^4.0.0",
        "commandbox-dotenv":"*",
        "commandbox-migrations":"*",
        "commandbox-cfconfig":"*",
        "commandbox-cfformat":"*",
        "cfmigrations":"^2.0.0"
    },
    "installPaths":{
        "coldbox":"coldbox/",
        "testbox":"testbox/",
        "quick":"modules/quick/",
        "cbauth":"modules/cbauth/",
        "cbguard":"modules/cbguard/",
        "BCrypt":"modules/BCrypt/",
        "cbvalidation":"modules/cbvalidation/",
        "redirectBack":"modules/redirectBack/",
        "cfmigrations":"modules/cfmigrations/",
        "cbInertia":"modules/cbInertia/"
